<%= simple_form_for (@cocktail) do |form| %>
<div class="form-group">
  <%= form.label :name %>
  <%= form.text_field :name, class: 'form-control' %>
</div>
<div class="form-group">
 <%= form.input :photo %>
  <%= form.input :photo_cache, as: :hidden %>
  </div>
<%= form.submit class: 'btn btn-primary'  %>
</div>
<% end %>
